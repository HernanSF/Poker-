{
  "name": "poker",
  "version": "1.0.0",
  "description": "poker game",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=lcov mocha -r ts-node/register ./src/test/**/*.test.ts",
    "coverage": "nyc --reporter=html mocha -r ts-node/register ./src/test/**/*.test.ts",
    "lint": "tslint -c ./tslint.json './src/**/*.ts'"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HernanSF/Poker-.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HernanSF/Poker-/issues"
  },
  "homepage": "https://github.com/HernanSF/Poker-#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.2.0",
    "tslint": "^5.18.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.5.2"
  }
}
